{
  "hosting": {
    "public": "web-build",
    "headers": [
      { "source":"**", "headers": [{"key": "Cache-Control", "value": "max-age=7200"}] }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
