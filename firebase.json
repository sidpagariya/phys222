{
  "hosting": {
    "public": "web-build",
    "headers": [
      {
        "source": "**/*.@(html|js)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=60" }]
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
